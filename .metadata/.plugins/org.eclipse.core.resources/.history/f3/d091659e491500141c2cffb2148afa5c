package com.money.captainbird.physicsworld;

import org.andengine.extension.physics.box2d.PhysicsWorld;

import com.badlogic.gdx.math.Vector2;

public class Physics {
	public static PhysicsWorld p;
	private static final float ACCEL_X = 0f;
	private static final float ACCEL_Y = -100f;
	
	public Physics() {
		Physics.p = new PhysicsWorld(new Vector2(ACCEL_X,ACCEL_Y), false);
	}
}
