package com.money.captainbird.EntityData;

public class Entity {
	private String type;
	private int posX;
	private int posY;
	private String src;
	public Entity(String type, int posX, int posY, String src) {
		super();
		this.type = type;
		this.posX = posX;
		this.posY = posY;
		this.src = src;
	}
	public String getType() {
		return type;
	}
	public int getPosX() {
		return posX;
	}
	public int getPosY() {
		return posY;
	}
	public String getSrc() {
		return src;
	}
}
