package com.money.captainbird.EntityData;

public class Entity {
	private String type;
	private int posX;
	private int posY;
	private String src;
	
	public Entity() {
		super();
	}
	public String getType() {
		return type;
	}
	public int getPosX() {
		return posX;
	}
	public int getPosY() {
		return posY;
	}
	public String getSrc() {
		return src;
	}
}
